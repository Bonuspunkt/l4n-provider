{
  "name": "l4n-provider",
  "version": "1.0.0-rc.3",
  "bin": {
    "l4n-provider": "./bin/l4n-provider.js"
  },
  "description": "// TODO: rough routline",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "http-router": "^0.5.0",
    "l4n-common": "^0.0.0",
    "repatch": "^1.3.11",
    "tree-kill": "^1.2.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "4.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "prettier": "^1.7.4",
    "tap": "^10.7.2"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore  .",
    "test": "tap **.test.js"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "parserOptions": {
      "ecmaVersion": 2017,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "extends": [
      "eslint:recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "none"
        }
      ],
      "prettier/prettier": [
        "error",
        {
          "printWidth": 100,
          "singleQuote": true,
          "tabWidth": 4,
          "trailingComma": "all"
        }
      ]
    }
  },
  "author": "Bonuspunkt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bonuspunkt/l4n-provider.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/Bonuspunkt/l4n-provider/issues"
  },
  "homepage": "https://github.com/Bonuspunkt/l4n-provider#readme"
}
